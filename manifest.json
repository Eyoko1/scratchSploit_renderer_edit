{
    "manifest_version": 3,
    "name": "Scratch sploit",
    "version": "2.0.0",
    "content_scripts": [
        {
            "matches": ["*://scratch.mit.edu/*"],
            "run_at":"document_start",
            "js": ["main.js"],
            "world": "MAIN"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "*://scratch.mit.edu/*"
    ]
}